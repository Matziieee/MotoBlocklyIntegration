<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.blocklywebview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.google.blockly.android.webview.demo.Activities.GameCollectionActivity"
            android:exported="false" />
        <activity
            android:name="com.google.blockly.android.webview.demo.Activities.BlocklyGameActivity"
            android:exported="false" />
        <activity
            android:name="com.google.blockly.android.webview.demo.Activities.HomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.google.blockly.android.webview.demo.Activities.BlocklyActivity"></activity>
        <activity android:name="com.google.blockly.android.webview.demo.Activities.CreateGamesActivity"></activity>
        <activity android:name="com.google.blockly.android.webview.demo.Activities.PracticeGamesActivity"></activity>
    </application>

</manifest>